{
  "name": "i18n",
  "version": "1.0.0",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.mjs",
      "types": "./dist/*/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "private": true,
  "scripts": {
    "build": "tsup --publicDir",
    "dev": "dotenv -e .env -- tsup --watch --publicDir"
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "i18next": "^22.5.0",
    "i18next-browser-languagedetector": "^7.0.2",
    "i18next-resources-to-backend": "^1.1.4",
    "next": "^13.4.1",
    "react-i18next": "^12.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.9",
    "dotenv-cli": "^7.2.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "tsconfig": "*",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  }
}
