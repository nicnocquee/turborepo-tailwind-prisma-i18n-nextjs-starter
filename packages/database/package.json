    "lint": "eslint \"src/**/*.ts\"",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prisma": "^4.14.1",
    "rimraf": "^3.0.2",
    "tsconfig": "*",
    "tsup": "^6.7.0",
    "tsx": "^3.7.1",
    "typescript": "^5.1.3"
  }
